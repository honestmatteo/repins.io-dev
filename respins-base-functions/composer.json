{
    "name": "respins/base-functions",
    "description": "Respins.io base functions",
    "keywords": [
        "respins",
        "laravel",
        "base-functions"
    ],
    "homepage": "https://github.com/respins/base-functions",
    "license": "MIT",
    "authors": [
        {
            "name": "Respins.io",
            "email": "fly@respins.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "ext-json": "*",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Respins\\BaseFunctions\\": "src",
            "Respins\\BaseFunctions\\Database\\Factories\\": "database/factories"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Respins\\BaseFunctions\\BaseFunctionsServiceProvider"
            ],
            "aliases": {
                "BaseFunctions": "Respins\\BaseFunctions\\Facades\\BaseFunctions"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
